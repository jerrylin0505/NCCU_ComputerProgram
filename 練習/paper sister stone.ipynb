    "import random #匯入random模組\n",
    "\n",
    "player = input(\"請出[0]拳 [1]剪刀[2]布\")\n",
    "int(player)\n",
    "cpu = random.randint(0,2) #亂數回0~2 區間內的一個整數\n",
    "\n",
    "print(\"你出的:\",player)\n",
    "print(\"電腦出的\",cpu)\n",
    "\n",
    "if player == cpu:\n",
    "    print(\"平手\")\n",
    "elif player == (cpu + 1)%3:\n",
    "    print(\"玩家贏\")\n",
    "else:\n",
    "    print(\"電腦贏\")"
   ]
